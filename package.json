  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "@prisma/client": "3.14.0",
    "blitz": "0.45.4",
    "classnames": "^2.3.1",
    "dotenv-flow": "3.2.0",
    "esbuild": "^0.14.39",
    "final-form": "4.20.7",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-final-form": "6.5.9",
    "tailwind-styled-components": "^2.1.7",
    "zod": "3.17.3"
