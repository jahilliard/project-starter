    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.6.2",
    "prettier-plugin-prisma": "3.14.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "prisma": "3.14.0",
    "run-p": "0.0.0",
    "run-s": "0.0.0",
    "tailwindcss": "3.0.24",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~4.5"
